<!DOCTYPE html>
<html>
<head>
  <title>Property Management</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	
	<!-- Begin Wrapper -->
<div id="wrapper">

  <!-- Begin Header -->
  <div id="header">

   Allieson Serd's Awesome Assignment 6 Property Management Page

  </div>
  <!-- End Header -->

  <!-- Begin Navigation -->
  <div id="navigation">
	<div id="login">
	
		<%if current_user %>
		
			<%= link_to "Logout", logout_path%>
		<% else %>
			<%= link_to "Register", new_user_path%>
			<%= link_to "Login", login_path%>
			
		<% end %>	
</div>
 
  </div>
  <!-- End Navigation -->

  <!-- Begin Content -->
  <div id="content">

	<% if flash[:error] -%>
		<div id='error'><%=h flash[:error] %> </div>
	<% end -%>
	<% if flash[:notice] -%>
		<div id='notice'><%=h flash[:notice] %> </div>
	<% end -%>

<%= yield %>

</div>
  <!-- End Content -->
  <!-- Begin Footer -->
  <div id="footer">

    Copyright 2013, Property Management System, Inc. Allieson Serd

  </div>
  <!-- End Footer -->

</div>
<!-- End Wrapper -->

</body>
</html>
